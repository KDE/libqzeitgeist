set(PACKAGE_VERSION @QZEITGEIST_VERSION@)

if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL @QZEITGEIST_VERSION_MAJOR@)
    set(PACKAGE_VERSION_COMPATIBLE 1)
    if("${PACKAGE_FIND_VERSION_MINOR}" EQUAL @QZEITGEIST_VERSION_MINOR@)
        set(PACKAGE_VERSION_EXACT 1)
    endif()
endif()

if("${PACKAGE_FIND_VERSION_MAJOR}" EQUAL 0 AND "${PACKAGE_FIND_VERSION_MINOR}" EQUAL 7)
    set(PACKAGE_VERSION_COMPATIBLE 0)
endif()
